@MAKE_POSIX_TARGET@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
MK_VPATH = @srcdir@
INSTALL = @INSTALL@

@mk_head@
@mk_dirs@

INCS = -I@top_builddir@/src/include -I$(top_srcdir)/src/include

SCHD_CONFIG = sched_config
CFLAGS = @CFLAGS@ $(INCS) @DEFS@ -DCONFIGFILE=\"$(PBS_SERVER_HOME)/sched_priv/$(SCHD_CONFIG)\"

TARGET = pbs_sched.a

OBJS =  acl_support.o cleanup.o comment.o evaluate_system.o fair_access.o  \
	getconfig.o getjobs.o getqueues.o getrsrcs.o jobinfo.o msgs.o \
	pack_queues.o  queue_limits.o resource_limits.o runjob.o schedinit.o \
	schedule.o time_limits.o toolkit.o user_limits.o usersort.o

@mk_lib@

install:: build

install::
	@echo Installing scheduler config file
	@for f in $(SCHD_CONFIG); do \
	    test -f $(PBS_SERVER_HOME)/sched_priv/$$f || \
	        $(INSTALL) -m 644 $(srcdir)/$$f $(PBS_SERVER_HOME)/sched_priv; \
	done

@mk_cleanup@
@mk_tail@

